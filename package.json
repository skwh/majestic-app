{
  "name": "majestic-app",
  "version": "1.0.0",
  "description": "Web application for Dr. Brian Majestic of the University of Denver Chemistry Department.",
  "private": true,
  "main": "app.js",
  "scripts": {
    "build:dev": "./build/build.sh false",
    "build:prod": "./build/build.sh true",
    "docker-node-dev": "npm run build:dev && npm run node-dev",
    "node-dev": "nodemon src/server.js",
    "serve": "npm run build:prod && node dist/server.js",
    "test": "npm run test:api && npm run test:vue",
    "test:api": "mocha test/api/ --color --exit --file test/api/setup.js",
    "test:api:watch": "mocha test/api/ --color --file test/api/setup.js --watch",
    "test:vue": "mocha-webpack --webpack-config webpack.config.js --require test/view/setup.js --exit test/view/**/*.spec.js",
    "test:vue:watch": "mocha-webpack --watch --webpack-config webpack.config.js --require test/view/setup.js test/view/**/*.spec.js",
    "vue-dev": "webpack-dev-server --env.production=false --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skwh/majestic-app.git"
  },
  "author": "Evan Derby <evan.derby@du.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skwh/majestic-app/issues"
  },
  "homepage": "https://github.com/skwh/majestic-app#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "chart.js": "^2.8.0",
    "chartjs-plugin-streaming": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "helmet": "^3.21.2",
    "moment": "^2.24.0",
    "pg": "^7.12.1",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "canvas": "^2.6.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-spies": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "expect": "^24.9.0",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^15.2.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "moment-locales-webpack-plugin": "^1.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.4",
    "null-loader": "^3.0.0",
    "sass": "^1.23.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.2",
    "webpack-node-externals": "^1.7.2"
  }
}
